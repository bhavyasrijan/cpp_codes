//Q21


/*#include<iostream>
using namespace std;

int main(){

int n,i,j;
cout<<"enter n to print hollow butterfly pattern of 2n rows"<<"\n";
cin>>n;

for(i=1;i<=n;i++){
   int empty_space=2*n-2*i;

   for(j=1;j<=i;j++){
       if(j==1||j==i)
       cout<<"*";
       
       else
       cout<<" ";
   }

   for(j=1;j<=empty_space;j++){
       cout<<" ";

   }

   for(j=1;j<=i;j++){
       if(j==1||j==i)
       cout<<"*";
       else
       cout<<" ";
   }

   cout<<"\n";

}

for(i=n;i>=1;i--){
   int empty_space=2*n-2*i;

   for(j=1;j<=i;j++){
       if(j==1||j==i)
       cout<<"*";
       
       else
       cout<<" ";
   }

   for(j=1;j<=empty_space;j++){
       cout<<" ";

   }

   for(j=1;j<=i;j++){
       if(j==1||j==i)
       cout<<"*";
       else
       cout<<" ";
   }

   cout<<"\n";
}


return 0;
}*/



//Q22 pascal's triangle


/*#include<iostream>
using namespace std;

int main(){
int n,i,j;
cout<<"enter n for getting pascal's triangle of n rows"<<"\n";
cin>>n;
int num=1;

for(i=0;i<n;i++){

  for(int space=1;space<=n-i;space++){

      cout<<"  ";                         //double space
  }
  
  for(j=0;j<=i;j++){

      if(j==0||i==0)
      num=1;

      else
      num=num*(i-j+1)/j;

      cout<<num<< "   ";                //triple space
  }



   cout<<"\n";




}



return 0;
}*/



//Q23


/*#include<iostream>
using namespace std;

int main(){

int i,j,n;

cout<<"enter width of 3 rowed zig zag pattern"<<"\n";
cin>>n;

for(i=1;i<=3;i++){

    for(j=1;j<=n;j++){
        if(((i+j)%4==0)||((i==2)&&(j%4==0)))
        cout<<" *";                   //single space print before printing *

        else
        {
            cout<<"  ";             //2 spaces
        }
    }
    cout<<"\n";
}

return 0;
}*/












