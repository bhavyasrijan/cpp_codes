//Q1

#include<iostream>
using namespace std;

int main(){
    int m,n;
    cout<<"of how many rows star pattern you want"<<endl;
    cin>>m;
    cout<<"of how many columns star pattern you want"<<endl;
    cin>>n;
    for(int i=0;i<m;i++){
        for(int j=0;j<n;j++){
            cout<<"*";
           
        }
         cout<<endl;
    }
return 0;
}


