//method-2


/*#include<iostream>
using namespace std;

int main(){
int i,j,n;
cout<<"enter n to get butterfly pattern of 2n rows"<<"\n";
cin>>n;

for ( i = 1; i <=n; i++)
{
    int empty_space=2*n-2*i;

    for(j=1;j<=i;j++){
        cout<<"*";
    }

    for(j=1;j<=empty_space;j++){
        cout<<" ";

    }

    for(j=1;j<=i;j++){
        cout<<"*";
    }

    cout<<"\n";
}

for ( i = n; i >=1; i--)
{
    int empty_space=2*n-2*i;

    for(j=1;j<=i;j++){
        cout<<"*";
    }

    for(j=1;j<=empty_space;j++){
        cout<<" ";

    }

    for(j=1;j<=i;j++){
        cout<<"*";
    }

    cout<<"\n";
}


return 0;
